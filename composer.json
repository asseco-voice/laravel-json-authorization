{
    "name": "asseco-voice/laravel-json-authorization",
    "description": "Laravel JSON authorization",
    "keywords": [
        "php",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "asseco-voice/laravel-json-query-builder": "dev-master",
        "illuminate/support": "^7.17",
        "illuminate/database": "^7.17",
        "illuminate/http": "^7.17"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "mockery/mockery": "^1.3.1",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Voice\\JsonAuthorization\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Voice\\JsonAuthorization\\JsonAuthorizationServiceProvider"
            ]
        }
    }
}
